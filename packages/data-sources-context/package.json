{
  "version": "0.1.11",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "yarn --cwd=../.. test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "if-env PREPARE_NO_BUILD=true || yarn build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@plasmicapp/data-sources-context",
  "author": "Chung Wu",
  "module": "dist/data-sources-context.esm.js",
  "size-limit": [
    {
      "path": "dist/data-sources-context.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/data-sources-context.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.4",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "husky": "^7.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-banner2": "^1.2.2",
    "size-limit": "^7.0.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
