{
  "version": "1.0.35",
  "license": "MIT",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "yarn build:types && yarn build:index",
    "build:types": "yarn tsc",
    "build:index": "node ../../build.mjs ./src/index.ts",
    "yalcp": "yalc publish --push",
    "test": "yarn --cwd=../.. test --passWithNoTests",
    "coverage": "yarn --cwd=../.. test --coverage --passWithNoTests",
    "lint": "eslint",
    "prepare": "if-env PREPARE_NO_BUILD=true || yarn build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "name": "@plasmicapp/loader-edge",
  "author": "Chung Wu",
  "size-limit": [
    {
      "path": "./dist/index.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "@plasmicapp/loader-fetcher": "1.0.29",
    "@plasmicapp/loader-splits": "1.0.34"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "fa53f7d79f0e26d8b061102fda0c06788da6f8a7"
}
