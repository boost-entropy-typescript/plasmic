{
  "name": "plume-stories",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@plasmicapp/react-web": "^0.2.229",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "react": "link:../react-web/node_modules/react",
    "react-dom": "link:../react-web/node_modules/react-dom",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "cypress run --component",
    "cypress": "cypress open --component",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@cypress/react": "^7.0.1",
    "@cypress/webpack-dev-server": "^1.1.6",
    "@storybook/addon-actions": "^6.2.8",
    "@storybook/addon-essentials": "^6.2.8",
    "@storybook/addon-links": "^6.2.8",
    "@storybook/node-logger": "^6.2.8",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.2.8",
    "@testing-library/cypress": "^8.0.7",
    "cypress": "^11.2.0",
    "cypress-real-events": "^1.7.4",
    "local-cypress": "^1.2.1",
    "webpack-dev-server": "^3.11.2"
  }
}
