{
  "name": "@plasmicpkgs/data-table",
  "version": "1.0.5",
  "description": "Table component to consume query data",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/data-table.esm.js",
  "files": [
    "dist"
  ],
  "size-limit": [
    {
      "path": "dist/data-table.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/data-table.esm.js",
      "limit": "10 KB"
    }
  ],
  "scripts": {
    "build": "tsdx build",
    "start": "tsdx watch",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "if-env PREPARE_NO_BUILD=true || yarn build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "devDependencies": {
    "@plasmicapp/host": "1.0.91",
    "@size-limit/preset-small-lib": "^4.11.0",
    "@types/node": "^14.0.26",
    "size-limit": "^4.11.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@plasmicapp/data-sources": "0.1.24"
  },
  "peerDependencies": {
    "@plasmicapp/host": "^1.0.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
