{
  "name": "@plasmicpkgs/plasmic-keen-slider",
  "version": "0.0.3",
  "description": "Plasmic Keen slider components.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/plasmic-keen-slider.esm.js",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "yarn --cwd=../.. test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "if-env PREPARE_NO_BUILD=true || yarn build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "size-limit": [
    {
      "path": "dist/plasmic-keen-slider.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/plasmic-keen-slider.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@plasmicapp/host": "1.0.161",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1"
  },
  "dependencies": {
    "@seznam/compose-react-refs": "^1.0.6",
    "antd": "^5.8.4",
    "keen-slider": "^6.8.6"
  }
}
