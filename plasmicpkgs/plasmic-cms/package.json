{
  "name": "@plasmicpkgs/plasmic-cms",
  "version": "0.0.115",
  "description": "Plasmic CMS components",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/plasmic-cms.esm.js",
  "files": [
    "dist"
  ],
  "size-limit": [
    {
      "path": "dist/plasmic-cms.cjs.production.min.js",
      "limit": "20 KB"
    },
    {
      "path": "dist/plasmic-cms.esm.js",
      "limit": "20 KB"
    }
  ],
  "scripts": {
    "build": "tsdx build",
    "start": "tsdx watch",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "if-env PREPARE_NO_BUILD=true || yarn build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "devDependencies": {
    "@plasmicapp/host": "1.0.74",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/node": "^17.0.14",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@plasmicapp/query": "0.1.53",
    "dayjs": "^1.10.7"
  },
  "peerDependencies": {
    "@plasmicapp/host": "^1.0.54",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  }
}
