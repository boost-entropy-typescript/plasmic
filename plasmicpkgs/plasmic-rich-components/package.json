{
  "name": "@plasmicpkgs/plasmic-rich-components",
  "version": "1.0.48",
  "description": "Rich batteries-included general purpose components for business apps, admin panels, etc.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/plasmic-rich-components.esm.js",
  "files": [
    "dist"
  ],
  "size-limit": [
    {
      "path": "dist/plasmic-rich-components.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/plasmic-rich-components.esm.js",
      "limit": "10 KB"
    }
  ],
  "scripts": {
    "build": "tsdx build",
    "start": "tsdx watch",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "if-env PREPARE_NO_BUILD=true || yarn build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "devDependencies": {
    "@ant-design/icons": "^5.0.1",
    "@ant-design/pro-components": "^2.4.1",
    "@plasmicapp/data-sources": "0.1.62",
    "@plasmicapp/host": "1.0.124",
    "@size-limit/preset-small-lib": "^4.11.0",
    "@types/node": "^14.0.26",
    "antd": "^5.4.0",
    "size-limit": "^4.11.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0",
    "typescript": "^3.9.7"
  },
  "peerDependencies": {
    "@ant-design/icons": ">=5.0.0",
    "@ant-design/pro-components": ">=2.4.1",
    "@plasmicapp/data-sources": ">=0.1.52",
    "@plasmicapp/host": ">=1.0.0",
    "antd": ">=5.0.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "csv-writer-browser": "^1.0.0",
    "fast-stringify": "^2.0.0",
    "lodash": "^4.17.21"
  }
}
