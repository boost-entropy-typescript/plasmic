{
  "name": "@plasmicpkgs/react-chartjs-2",
  "version": "1.0.44",
  "description": "Chart.js 2.x components for React",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/react-chartjs-2.esm.js",
  "files": [
    "dist"
  ],
  "size-limit": [
    {
      "path": "dist/react-chartjs-2.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-chartjs-2.esm.js",
      "limit": "10 KB"
    }
  ],
  "scripts": {
    "build": "tsdx build",
    "start": "tsdx watch",
    "test": "yarn --cwd=../.. test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "if-env PREPARE_NO_BUILD=true || yarn build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "devDependencies": {
    "@plasmicapp/data-sources": "0.1.100",
    "@plasmicapp/host": "1.0.155",
    "@size-limit/preset-small-lib": "^4.11.0",
    "@types/node": "^14.0.26",
    "chart.js": "^4.2.1",
    "react-chartjs-2": "^5.2.0",
    "size-limit": "^4.11.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0"
  },
  "peerDependencies": {
    "@plasmicapp/data-sources": ">=0.1.52",
    "@plasmicapp/host": ">=1.0.0",
    "chart.js": ">=4.2.1",
    "react": ">=16.8.0",
    "react-chartjs-2": ">=5.2.0",
    "react-dom": ">=16.8.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "deepmerge": "^4.3.1"
  }
